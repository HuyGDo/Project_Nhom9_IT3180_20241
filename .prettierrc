{
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "all",
    "semi": true,
    "singleQuote": false,
    "printWidth": 100,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "arrowParens": "always",
    "overrides": [
        {
            "files": "**/*.hbs",
            "options": {
                "parser": "angular"
            }
        }
    ]
}
